{
  "manifest_version": 2,
  "name": "Twitter Notifications",
  "description": "Desktop Notifications of twitter notifications",
  "background":{
    "page":"background.html"
  },
  "version":"1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons" :{
    "128" : "icon.png"
  },
  "permissions":[
    "notifications",
    "https://twitter.com/i/notifications"
  ]
}